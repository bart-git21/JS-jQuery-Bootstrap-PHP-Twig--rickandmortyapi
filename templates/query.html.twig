{
  characters(page: 1, filter: { name: "{{name}}", status: "{{status}}", gender: "{{gender}}", species: "{{species}}",  }) {
    info {
      count
    }
    results {
      name
    }
  }
  location(id: 1) {
    id
    name
  }
  episodesByIds(ids: [{% for id in ids %} {{ id }}, {% endfor %}]) {
    id
    name
  }
}